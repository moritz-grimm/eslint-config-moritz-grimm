{
    "name": "@moritz-grimm/eslint-config-moritz-grimm",
    "version": "1.2.0",
    "description": "My preferred eslint config",
    "keywords": [
        "eslint",
        "eslintconfig",
        "config"
    ],
    "homepage": "https://github.com/Moritz-Grimm/eslint-config-moritz-grimm#readme",
    "bugs": {
        "url": "https://github.com/Moritz-Grimm/eslint-config-moritz-grimm/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Moritz-Grimm/eslint-config-moritz-grimm.git"
    },
    "license": "MIT",
    "author": "moritz-grimm",
    "type": "module",
    "exports": {
        ".": "./eslint.config.js",
        "./base": "./configs/eslint.base.config.js",
        "./typescript": "./configs/eslint.typescript.config.js",
        "./stylistic": "./configs/eslint.stylistic.config.js"
    },
    "main": "./eslint.config.js",
    "files": [
        "eslint.config.js",
        "configs/*",
        "LICENSE",
        "README.md"
    ],
    "scripts": {
        "lint": "npx eslint",
        "lint:fix": "npx eslint --fix",
        "publish:all": "npm publish --registry https://registry.npmjs.org && npm publish --registry https://npm.pkg.github.com/",
        "sort-package-json": "npx sort-package-json"
    },
    "dependencies": {
        "@eslint/js": "^9.37.0",
        "@stylistic/eslint-plugin": "^5.4.0",
        "globals": "^16.4.0",
        "jiti": "^2.6.1",
        "typescript-eslint": "^8.46.1"
    },
    "peerDependencies": {
        "eslint": ">=9",
        "typescript": ">=5.0.0"
    },
    "publishConfig": {
        "access": "public"
    }
}